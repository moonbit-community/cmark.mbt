{
  "import": [
    "rami3l/cmark/char",
    "rami3l/cmark/cmark_base",
    "rami3l/cmark/cmark",
    "rami3l/cmark/cmark_renderer"
  ],
  "pre-build": [
    {
      "input": "../data/test/basic.md",
      "output": "test_data_basic_test.mbt",
      "command": ":embed -i $input -o $output --name test_basic_md_str"
    },
    {
      "input": "../data/test/spec.md",
      "output": "test_data_spec_test.mbt",
      "command": ":embed -i $input -o $output --name test_spec_md_str"
    }
  ]
}
